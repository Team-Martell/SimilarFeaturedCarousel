

{
  "tags": {
      "allowUnknownTags": true
  },
  "source": {
      "include": ["./client"]
  },
  "plugins": [
      "plugins/markdown",
      "better-docs/component"
  ],
  "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/better-docs",
      "readme": "README.md"
  },
  "templates": {
      "better-docs": {
          "name": "My React components",
          "component": {
            "wrapper": "./client/src/components/Wrapper.jsx"
          }
      }
  }
}